" Configure for YouCompleteMe (see tag-mac/vimrc.bundles.arch)
" We don't want to run it on Python code; it fails at Python 3
let g:ycm_filetype_blacklist = {
	\ 'python': 1,
	\ 'tagbar' : 1,
	\ 'qf' : 1,
	\ 'notes' : 1,
	\ 'markdown' : 1,
	\ 'unite' : 1,
	\ 'text' : 1,
	\ 'vimwiki' : 1,
	\ 'pandoc' : 1,
	\ 'infolog' : 1,
	\ 'mail' : 1
	\}
